{
  // Plugin ka display name
  "name": "NetMirror",

  // Full path of the main plugin class
  "pluginClassName": "com.example.NetMirrorPlugin",

  // Short description
  "description": "NetMirror CloudStream plugin for streaming movies.",

  // Authors list
  "authors": ["MalikAsadullah"],

  // Plugin version (integer)
  "version": 1,

  // Supported content types (Movie, TvShow, etc.)
  "supportedTypes": ["Movie"],

  // Plugin status: 0=Down, 1=Ok, 2=Slow, 3=Beta-only
  "status": 1,

  // Plugin language code
  "language": "en",

  // Icon URL (CloudStream will fetch this online)
  "iconUrl": "https://upload.wikimedia.org/wikipedia/commons/2/2f/Korduene_Logo.png",

  // Indicates if plugin requires resources
  "requiresResources": true
}
